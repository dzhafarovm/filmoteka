{{#each this}}
<li class='gallery-item'>
   <a href='' onclick="event.preventDefault()" class='card-link' data-id="{{id}}">
      <div class='film-page'>
         <img class='film-image' src='https://image.tmdb.org/t/p/w500{{poster_path}}' alt='{{overview}}'
            loading='lazy' />
         <div class='film-info'>
            <p class='film-name'>{{title}}</p>
            <p class='film-genre'>{{genre_ids}} | {{release_date}}</p>
         </div>
      </div>
   </a>
</li>
{{/each}}